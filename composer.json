{
    "name": "spatie/laravel-url-signer",
    "description": "Laravel implementation of spatie/signed-url",
    "keywords": [
        "spatie",
        "laravel-url-signer"
    ],
    "homepage": "https://github.com/spatie/laravel-url-signer",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.1",
        "spatie/url-signer": "v2.x-dev",
        "illuminate/support": "^9.0",
        "illuminate/http": "^9.0",
        "spatie/laravel-package-tools": "^1.13"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\UrlSigner\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\UrlSigner\\Laravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\UrlSigner\\Laravel\\UrlSignerServiceProvider"
            ],
            "aliases": {
                "UrlSigner": "Spatie\\UrlSigner\\Laravel\\Facades\\UrlSigner"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
